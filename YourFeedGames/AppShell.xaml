<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="YourFeedGames.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:YourFeedGames"
    Shell.FlyoutBehavior="Flyout"
    Title="YourFeedGames">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Hot News"
        ContentTemplate="{DataTemplate local:HotNewsPage}"
        Route="HotNewsPage" />

    <ShellContent
        Title="Configurações"
        ContentTemplate="{DataTemplate local:SettingsPage}"
        Route="SettingsPage" />

    <ShellContent
         Title="Eventos"
         ContentTemplate="{DataTemplate local:EventsPage}"
         Route="EventsPage" />
</Shell>

